   "source": [
    "def max_subarrary(nums: Sequence[float]) -> float:\n",
    "  best_sum_before_n = maxsum = nums[0]\n",
    "  for n in nums[1:]:\n",
    "    best_sum_before_n = max(n, best_sum_before_n + n)\n",
    "    maxsum = max(maxsum, best_sum_before_n)\n",
    "  return maxsum\n",
    "\n",
    "# Dynamic programming"
   ]
  }
